{
  "type": "AppSource App",
  "templateUrl": "https://github.com/microsoft/AL-Go-AppSource@preview",
  "templateSha": "9aa981fe7c6f2e916d2f13485662ab8a9cf1ddb7",
  "rulesetFile": "../sr365.ruleset.json",
  "useProjectDependencies": true,
  "alwaysBuildAllProjects": false,
  "enableCodeCop": true,
  "enableUICop": true,
  "enableAppSourceCop": true,
  "failOn": "error",
  "cacheKeepDays": 10,
  "doNotBuildTests": false,
  "doNotSignApps": false,
  "BcContainerHelperVersion": "preview",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*"
  ],
  "CICDPullRequestBranches": [
    "main"
  ],
  "skipUpgrade": false,
  "ConditionalSettings": [
    {
      "branches": [
        "feature/*"
      ],
      "settings": {
        "doNotSignApps": true
      }
    }
  ]
}
